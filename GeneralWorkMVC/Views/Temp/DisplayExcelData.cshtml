@{
    ViewBag.Title = "Excel Data";
}

<h2>Sheet1 Data</h2>
<table border="1">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.Sheet1Data as List<GeneralWorkMVC.Models.User>)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Age</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>

<h2>Sheet2 Data</h2>
<table border="1">
    <thead>
        <tr>
            <th>Product</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in ViewBag.Sheet2Data as List<GeneralWorkMVC.Models.Product>)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.Price</td>
                <td>@product.Quantity</td>
            </tr>
        }
    </tbody>
</table>
